<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>list project</title>
  </head>
  <body>
    <h3>list project</h3>
    {{#each dataProjects}}
      <p>{{this.title}}</p>
      <p>{{this.description}}</p>
    {{/each}}

    {{! looping card project here }}
    {{!-- {{#each dataProjects}}
      <div class="card" style="width: 18rem;">
